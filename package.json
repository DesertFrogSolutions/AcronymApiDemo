{
  "name": "acronym-api",
  "version": "0.0.0",
  "description": "Acronym API Demo",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec --ui bdd **/*.spec.js",
    "test:watch": "mocha --reporter spec --ui bdd --watch **/*.spec.js",
    "test:live": "mocha --reporter spec --ui bdd src/live.test.js",
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "generate-initdb": "node src/generate-initdb.js > initdb.sql"
  },
  "author": "Jonathan Goldfarb <jgoldfar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bunyan": "1.8.15",
    "pg": "8.7.3",
    "restify": "8.6.1",
    "restify-errors": "8.0.2"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "mocha": "9.2.1",
    "nodemon": "2.0.15",
    "sinon": "13.0.1",
    "sqlutils": "1.4.0",
    "supertest": "6.2.2"
  }
}
